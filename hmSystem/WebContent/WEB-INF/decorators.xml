<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
    <excludes>
        <pattern>/login*</pattern>
        <pattern>/druid/*</pattern>
        <pattern>/WEB-INF/views/error/404.jsp</pattern>
    </excludes>

    <decorator name="default" page="default.jsp">
        <pattern>/*</pattern>
    </decorator>
</decorators>
